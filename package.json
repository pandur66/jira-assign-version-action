{
  "name": "jira-assign-version-action",
  "version": "1.0.0",
  "description": "Assigns a Jira version (by name or id) to one or more issues.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pandur66/jira-assign-version.git"
  },
  "homepage": "https://github.com/pandur66/jira-assign-version",
  "scripts": {
    "build": "tsc",
    "bundle": "ncc build dist/index.js -o dist",
    "lint": "eslint --ext .ts .",
    "format": "prettier --write .",
    "prepare": "npm run build && npm run bundle",
    "test": "vitest"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "@actions/http-client": "^2.2.0",
    "p-limit": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@vercel/ncc": "^0.38.1",
    "@types/node": "^20.0.0",
    "eslint": "^8.50.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "prettier": "^3.0.0",
    "eslint-config-prettier": "^9.0.0",
    "vitest": "^4.0.8"
  }
}
